    <!-- https://www.w3id.org/MALFO#Failure -->

    <owl:Class rdf:about="https://www.w3id.org/MALFO#Failure">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.w3id.org/MALFO#Event"/>

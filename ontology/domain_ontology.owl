        <owl:disjointWith rdf:resource="https://www.w3id.org/MALFO#FlowInstability"/>
        <owl:disjointWith rdf:resource="https://www.w3id.org/MALFO#HighBSWState"/>
        <owl:disjointWith rdf:resource="https://www.w3id.org/MALFO#HydrateInProductionLine"/>
        <owl:disjointWith rdf:resource="https://www.w3id.org/MALFO#HydrateInServiceLine"/>

        <rdfs:subClassOf rdf:resource="https://www.w3id.org/MALFO#State"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://www.w3id.org/MALFO#achieves"/>
                    </rdf:Description>

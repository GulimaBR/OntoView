            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://www.w3id.org/MALFO#Malfunction"/>
        <rdfs:subClassOf rdf:resource="https://www.w3id.org/MALFO#State"/>
        <owl:disjointWith rdf:resource="https://www.w3id.org/MALFO#Event"/>
